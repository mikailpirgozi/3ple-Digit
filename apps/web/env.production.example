# Frontend Production Environment Variables Template
# Copy this to .env.production and fill in real values

# API URL - Your Railway backend URL
VITE_API_URL="https://your-railway-app.railway.app"

# App Configuration
VITE_APP_NAME="3ple Digit"
VITE_APP_VERSION="1.0.0"

# Feature Flags
VITE_ENABLE_ANALYTICS="true"
VITE_ENABLE_ERROR_REPORTING="true"

# Development/Debug (set to false in production)
VITE_DEBUG="false"
